{
  "manifest_version": 2,

  "name": "GraphEx",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "0.1",

  "browser_action": {
    "default_icon": "/resources/toolbar/icon.png",
    "default_popup": "/src/toolbar/popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
